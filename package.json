    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.81.5",
    "@trpc/client": "^11.4.3",
    "@trpc/next": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "class-variance-authority": "^0.7.1",
